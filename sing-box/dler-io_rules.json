            },
            {
                "rule_set": [
                    "geoip-telegram",
                    "geosite-telegram"
                ],
                "outbound": "Telegram"
            },
            {
                "rule_set": "geosite-openai",
                "outbound": "AI Suite"
            },
            {
                "rule_set": "geosite-microsoft",
                "outbound": "Microsoft"
            },
            {
                "rule_set": "geosite-tiktok",
                "outbound": "TikTok"
            },
            {
                "rule_set": "geosite-private",
                "outbound": "Direct"
            },
            {
                "ip_is_private": true,
                "outbound": "Direct"
            },
            {
                "rule_set": [
                    "geoip-cn",
                    "geosite-cn"
                ],
                "outbound": "Domestic"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "outbound": "Others"
            }
        ],
